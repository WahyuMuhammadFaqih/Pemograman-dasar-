        "# program sederhana biodata dengan tipe data non primitif (list,tuple,set,dict)\n",
        "#===============================================================================\n",
        "\n",
        "# Biodata yang dipakai\n",
        "nama = \"Wahyu muhammad faqih\"\n",
        "umur = 19\n",
        "jurusan = \"Elektro\"\n",
        "prodi = \"Teknologi rekayasa pembangkit energi\"\n",
        "semester = 3\n",
        "status_lulus = False\n",
        "\n",
        "# tipe List\n",
        "biodata_list = [\"Wahyu muhammad faqih\", 19, \"Elektro\", \"Teknologi rekayasa pembangkit energi\", 3, False]\n",
        "#print langsung semua\n",
        "print(biodata_list)\n",
        "#print salah satu\n",
        "print(biodata_list[0])\n",
        "print(biodata_list[1])\n",
        "print(biodata_list[2])\n",
        "print(biodata_list[3])\n",
        "print(biodata_list[4])\n",
        "print(biodata_list[5])\n",
        "\n",
        "#tipe Tuple\n",
        "biodata_tuple = (\"Wahyu muhammad faqih\", 19, \"Elektro\", \"Teknologi rekayasa pembangkit energi\", 3, False)\n",
        "#print langsung semua\n",
        "print(biodata_tuple)\n",
        "#print salah satu\n",
        "print(biodata_tuple[0])\n",
        "print(biodata_tuple[1])\n",
        "print(biodata_tuple[2])\n",
        "print(biodata_tuple[3])\n",
        "print(biodata_tuple[4])\n",
        "print(biodata_tuple[5])\n",
        "\n",
        "#tipe set\n",
        "biodata_set = {\"Wahyu muhammad faqih\", 19, \"Elektro\", \"Teknologi rekayasa pembangkit energi\", 3, False}\n",
        "#print langsung semua (teracak)\n",
        "print(biodata_set)\n",
        "#loop print satu per satu (teracak)\n",
        "for data in biodata_set:\n",
        "  print(data)\n",
        "\n",
        "#tipe dictionary\n",
        "biodata_dictionary = {\"nama\": \"Wahyu muhammad faqih\", \"umur\": 19, \"jurusan\": \"Elektro\", \"prodi\": \"Teknologi rekayasa pembangkit energi\", \"semester\": 3, \"status_lulus\": False}\n",
        "#print langsung semua\n",
        "print(biodata_dictionary)\n",
        "#print satu per satu\n",
        "print(biodata_dictionary[\"nama\"])\n",
        "print(biodata_dictionary[\"umur\"])\n",
        "print(biodata_dictionary[\"jurusan\"])\n",
        "print(biodata_dictionary[\"prodi\"])\n",
        "print(biodata_dictionary[\"semester\"])\n",
        "print(biodata_dictionary[\"status_lulus\"])\n"
      ]
