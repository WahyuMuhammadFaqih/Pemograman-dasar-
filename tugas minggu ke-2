        "#program sederhana dengan menerapkan konsep annonation\n",
        "\n",
        "nama: str = \"Wahyu muhammad faqih\"\n",
        "umur: int = 19\n",
        "tinggi_badan: float = 175.0 #cm\n",
        "jurusan: str = \"Elektro\"\n",
        "prodi: str = \"Teknologi rekayasa pembbangkit energi\"\n",
        "semester: int = 3\n",
        "status_lulus: bool = False\n",
        "\n",
        "print(\"BIODATA MAHASISWA\")\n",
        "print(\"nama          :\",nama)\n",
        "print(\"umur          :\",umur,\"tahun\")\n",
        "print(\"tinggi badan  :\",tinggi_badan,\"cm\")\n",
        "print(\"jurusan       :\",jurusan)\n",
        "print(\"prodi         :\",prodi)\n",
        "print(\"semester      :\",semester)\n",
        "print(\"status lulus  :\",status_lulus)"
      ],
